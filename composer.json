{
    "name": "simplonprod/craft-google-recaptcha",
    "description": "Google reCAPTCHA for Craft CMS",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "google recaptcha"
    ],
    "support": {
        "docs": "https://github.com/juban/google-recaptcha/blob/master/README.md",
        "issues": "https://github.com/juban/google-recaptcha/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Simplon.Prod",
            "homepage": "https://www.simplonprod.co"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "simplonprod\\googlerecaptcha\\": "src/"
        }
    },
    "extra": {
        "name": "Google reCAPTCHA",
        "handle": "google-recaptcha",
        "developer": "Simplon.Prod",
        "developerUrl": "https://www.simplonprod.co",
        "documentationUrl": "https://github.com/juban/google-recaptcha/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/juban/google-recaptcha/master/CHANGELOG.md",
        "components": {
            "recaptcha": "simplonprod\\googlerecaptcha\\services\\Recaptcha"
        },
        "class": "simplonprod\\googlerecaptcha\\GoogleRecaptcha"
    }
}
